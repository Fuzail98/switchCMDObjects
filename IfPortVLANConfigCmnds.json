########### Port/Interface/VLAN basic configuration commands #############
1. {
    "name": "SET_IFDESC",
    "desc": "",
    "readOnly": false,
    "neId": "<NE_Master.id>",
    "cmdList": [
        {
            "cmd": "enable",
            "validRegEx": ".*#.*"
        },
        {
            "cmd": "config terminal",
            "validRegEx": ".*.*"
        },
        {
            "cmd": "interface __ifname__",
            "validRegEx": ".*.*"
        },
        {
            "cmd": "description __description__",
            "validRegEx": ".*.*"
        },
        {
            "cmd": "exit",
            "validRegEx": ".*.*"
        },
        {
            "cmd": "exit",
            "validRegEx": ".*.*"
        },
        {
            "cmd": "write",
            "validRegEx": ".*.*"
        }
    ],
    "args": [
        {
            "name": "description",
            "required": true
        },
        {
            "name": "ifname",
            "required": true
        }
    ]
}

